{
  "name": "@cmcrobotics/pronolab",
  "version": "0.7.3",
  "description": "A web UI orchestrator to assess machine learning models",
  "repository": "github:cmcrobotics/pronolab",
  "keywords": [
    "homie",
    "typescript",
    "iot",
    "machine-learning",
    "teachable-machine",
    "tensorflow"
  ],
  "scripts": {
    "build": "npm run build:lib && npm run build:core && npm run build:esm && npm run build:dev",
    "build:lib": "webpack --mode production --config webpack.lib.config.js",
    "build:core": "webpack --mode production --config webpack.lib.core.config.js",
    "build:esm": "webpack --mode production --config webpack.lib.esm.config.js",
    "build:dev": "webpack --mode development --config webpack.lib.dev.config.js",
    "build:view": "webpack --mode production --config webpack.view.config.js",
    "start": "run-p start:view start:server",
    "start:view": "webpack-dev-server --config webpack.dev.config.js",
    "start:server": "ts-node --project tsconfig.server.json src/server.ts",
    "init-teams": "node bin/init-teams.js"
  },
  "author": "Brice Copy",
  "license": "GPL-3.0-only",
  "devDependencies": {
    "@types/express": "^4.17.25",
    "@types/jest": "^29.5.13",
    "@types/loglevel": "^1.5.4",
    "@types/mqtt": "^0.0.34",
    "@types/multer": "^2.0.0",
    "@types/node": "^16.11.12",
    "@types/unzipper": "^0.10.11",
    "dotenv": "^16.4.5",
    "express": "^5.1.0",
    "jest": "^29.7.0",
    "multer": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.9.2",
    "typescript": "^4.5.4",
    "unzipper": "^0.12.3",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.2",
    "webpack-merge": "^6.0.1"
  },
  "dependencies": {
    "@teachablemachine/image": "0.8.3",
    "@teachablemachine/pose": "0.8.3",
    "@tensorflow-models/speech-commands": "0.4.0",
    "@tensorflow/tfjs": "1.3.1",
    "loglevel": "^1.9.2",
    "mqtt": "^5.10.1",
    "process": "^0.11.10",
    "rxjs": "^7.8.1",
    "util": "^0.12.5"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  }
}
